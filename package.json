{
	"name": "zeebo",
	"description": "Theme description",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"author": {
		"name": "Troy Ciesco",
		"email": "tmciesco@gmail.com",
		"url": "https://troycies.co"
	},
	"keywords": [
		"zeebo",
		"Troy Ciesco",
		"ghost",
		"ghost-theme"
	],
	"scripts": {
		"start": "postcss ./assets/css/tailwind.css -o ./assets/build/tailwind.css --minify --watch",
		"tailwind": "tailwindcss  -i ./assets/css/tailwind.css -o ./assets/build/tailwind.css --minify"
	},
	"screenshots": {
		"desktop": "assets/hello-world-thumb.png"
	},
	"config": {
		"image_sizes": {
			"xxs": {
				"width": 30
			},
			"xs": {
				"width": 100
			},
			"s": {
				"width": 300
			},
			"m": {
				"width": 600
			},
			"l": {
				"width": 1000
			},
			"xl": {
				"width": 2000
			}
		},
		"posts_per_page": 10,
		"card_assets": true
	},
	"dependencies": {
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"tailwindcss": "^3.3.1"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9"
	}
}
